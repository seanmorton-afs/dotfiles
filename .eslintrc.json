{
    "globals": {
        "angular": false,
        "mixpanel": false,
        "moment": false,
        "_": false
    },
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": ["error","always"],
        "no-undefined": "warn",
        "no-unused-vars": "warn",
        "no-empty": "warn",
        "no-redeclare": "warn",
        "camelcase": "warn",
        "linebreak-style": ["warn", "unix"],
        "no-use-before-define": "warn",
        "no-multiple-empty-lines": "warn",
        "no-shadow": "warn",
        "no-global-assign": "error",
        "no-fallthrough": "warn",
        "block-scoped-var": "error",
        "no-alert": "warn"
    }
}
